{
  "name": "phostalgia",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-addons-update": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "react-svg": "^2.1.21",
    "react-swipe": "^5.0.8"
  },
  "devDependencies": {
    "@google/maps": "^0.4.1",
    "axios": "^0.16.2",
    "exif-js": "^2.2.1",
    "node-sass-chokidar": "0.0.2",
    "npm-run-all": "^4.0.2",
    "react-redux": "^5.0.5",
    "react-scripts": "1.0.7",
    "redux": "^3.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/public/scss/ -o public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/public/scss/ -o public/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
